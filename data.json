			"fields": {
				"taste": "sweet",
				"header": "Red Velvet Cake",
				"picture": [
					{
						"sys": {
							"type": "Link",
							"linkType": "Asset",
							"id": "1sJ3CWrJhVv7cUz4sbU7dA",
                            "url": "//images.ctfassets.net/d6gach6xgkb5/1sJ3CWrJhVv7cUz4sbU7dA/02cfe89eee15a18f73ace68dfc73680f/persgroep"
						}
					}
				],
				"receiptText": {
					"data": {},
					"content": [
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [],
									"value": "For Cake:",
									"nodeType": "text"
								}
							],
							"nodeType": "heading-4"
						},
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [],
									"value": "Heat oven to 350°F | 175°C. Lightly grease two 8-inch cake pans with butter or nonstick oil spray and lightly dust with 1 tablespoon of the sifted cocoa powder.",
									"nodeType": "text"
								}
							],
							"nodeType": "paragraph"
						},
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [],
									"value": "Cream 1/2 cup (120g) unsalted butter and 1/2 cups (300g) caster sugar together until light in color. Add 2 large eggs, eggs one at a time, beating well after each addition to combine well.",
									"nodeType": "text"
								}
							],
							"nodeType": "paragraph"
						},
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [],
									"value": "In a smaller bowl, mix together 1/4cup cooking oil, 1 tablespoon (10g) unsweetened cocoa powder, 1/2 tablespoons (45ml) red food coloring and 2teaspoons(10ml) pure vanilla extract until smooth. Stir color mixture and 1tablespoon(20ml) white vinegar through the creamed sugar mixture to combine.",
									"nodeType": "text"
								}
							],
							"nodeType": "paragraph"
						},
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [],
									"value": "Sift together 2 1/2 cups (350g) plain cake flour sifted, 1 teaspoon baking soda and 1 teaspoon  salt in a separate bowl. Add half of the dry ingredients and half of the buttermilk to the wet ingredients; mix well. Repeat with remaining dry ingredients and buttermilk. ",
									"nodeType": "text"
								}
							],
							"nodeType": "paragraph"
						},
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [],
									"value": "Divide batter among the 2 prepared pans and bake for about 25 to 30 minutes, or until a toothpick inserted into the center of the cake comes out clean. Cool on a wire rack completely.",
									"nodeType": "text"
								}
							],
							"nodeType": "paragraph"
						},
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [
										{
											"type": "bold"
										}
									],
									"value": "For Cream Cheese Frosting:",
									"nodeType": "text"
								}
							],
							"nodeType": "paragraph"
						},
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [],
									"value": "Beat together cream cheese, butter and vanilla until smooth lighter in color (about 3-4 minutes). Beat in icing sugar until frosting is light and fluffy (if frosting is too thin, add more icing sugar and beat again until reaching your desired consistency).Optional if using: mix in the lemon juice.",
									"nodeType": "text"
								}
							],
							"nodeType": "paragraph"
						},
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [],
									"value": "Assemble Cake:",
									"nodeType": "text"
								}
							],
							"nodeType": "heading-4"
						},
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [],
									"value": "Transfer 1 cake onto a serving dish/plate, flat-side down. Trim the top dome off of the cake to create a flat bottom later. Scoop about 1 1/2 - 2 cups of frosting onto cake and spread evenly over the top.",
									"nodeType": "text"
								}
							],
							"nodeType": "paragraph"
						},
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [],
									"value": "Place second cake layer on top and use remaining frosting to cover top and sides of cake.",
									"nodeType": "text"
								}
							],
							"nodeType": "paragraph"
						},
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [],
									"value": "Crumble trimmed pieces of cake to decorate.",
									"nodeType": "text"
								}
							],
							"nodeType": "paragraph"
						},
						{
							"data": {},
							"content": [
								{
									"data": {},
									"marks": [],
									"value": "\n\n",
									"nodeType": "text"
								}
							],
							"nodeType": "paragraph"
						}
					],
					"nodeType": "document"
				}
			}
		},
